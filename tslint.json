{
  "extends": ["tslint-react"],
  "rules": {
    "class-name": true,
    "comment-format": [
      true,
      "check-space"
    ],
    "indent": [
      true,
      "spaces"
    ],
    "member-ordering": [
      true,
      {
        "order": "fields-first"
      }
    ],
    "only-arrow-functions": [
      true,
      "allow-declarations"
    ],
    "curly": true,
    "no-console": [
      true,
      "log"
    ],
    "eofline": true,
    "linebreak-style": [
      true,
      "LF"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "align": [
      true,
      "parameters",
      "statements"
    ],
    "no-consecutive-blank-lines": [
      true,
      1
    ],
    "ordered-imports": [
      true,
      {
        "import-sources-order": "case-insensitive",
        "named-imports-order": "case-insensitive"
      }
    ],
    "no-eval": true,
    "no-internal-module": true,
    "no-trailing-whitespace": true,
    "no-unsafe-finally": true,
    "no-var-keyword": true,
    "one-line": [
      true,
      "check-open-brace",
      "check-whitespace"
    ],
    "quotemark": [
      true,
      "double"
    ],
    "semicolon": [
      true,
      "always"
    ],
    "triple-equals": [
      true
    ],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "variable-name": [
      true,
      "ban-keywords"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ],
    "jsx-curly-spacing": false,
    "jsx-no-lambda": false
  }
}